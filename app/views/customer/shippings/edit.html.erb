<h3>配送先編集</h3>
<teble class="table">
  <%= form_for @shipping, url:"/shippings/#{@shipping.id}" do |f| %>
  <tr>
    <td>郵便番号(ハイフンなし)</td>
    <td><%= f.text_field :post_code %></td>
  </tr>
  <tr>
    <td>住所</td>
    <td><%= f.text_field :address %></td>
  </tr>
  <tr>
    <td>宛名</td>
    <td><%= f.text_field :name %></td>
    <td><%= f.submit '変更内容を保存' %></td>
  </tr>
  <% end %>
</teble>